   "source": [
    "### ESTIMATE STATE SEQUENCE FOR EACH TEST RECORDING\n",
    "test_files = glob.glob(\"audio/test*.mp3\") # retreive relevant files\n",
    "S_est_list = [] # Element i of S_est_list will be another list describing the state sequence of the i'th audio file in test_files\n",
    "\n",
    "for i in range(len(test_files)):    # iterate through files \n",
    "    S_est = estimateStateSequence(test_files[i], means=means, covars=covars, A=A, pi=pi) \n",
    "    S_est_list.append(S_est)    # append state sequence to list (can't use np array bc different sequence lengths)"
