   "id": "428b0631",
   "metadata": {},
   "outputs": [],
   "source": [
    "### DEFINE FUNCTION TO CONVERT INDICES TO TIMESTAMPS\n",
    "\n",
    "def convertToTimesteps(indices, hop_seconds):\n",
    "    timesteps = np.array(indices) * hop_seconds\n",
    "    return timesteps"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 218,
   "id": "216886e8",
   "metadata": {},
   "outputs": [],
   "source": [
    "### DEFINE FUNCTION TO ELIMINATE ADJACENT MULTIPLES FROM STATE SEQUENCE\n",
    "# this gives us a state sequence with no repetition, so each entry can be used as the \n",
    "# descriptor for a label\n",
    "\n",
    "def collapseStateSequence(S_est):\n",
    "    S_collapsed = [S_est[0]]\n",
    "    for i in range(1,len(S_est)):\n",
    "        if S_est[i] != S_est[i-1]:\n",
    "            S_collapsed.append(S_est[i])\n",
    "    return S_collapsed"
   ]
